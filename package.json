{
  "name": "50-state-marathons",
  "version": "1.0.0",
  "description": "Website for my dad's 50th marathon state",
  "main": "index.js",
  "engines": {
    "node": ">=8.9.4",
    "yarn": "1.5.1"
  },
  "repository": "https://github.com/marktkimball/50-state-marathons.git",
  "author": "marktkimball <mark.kimball@octanner.com>",
  "license": "MIT",
  "scripts": {
    "build": "rm -rf dist/ && webpack -p --colors",
    "lint": "yarn lint:js && yarn lint:ts",
    "lint:js": "eslint ./src --ext .js",
    "lint:ts": "tslint --project ./tsconfig.json ./src/**/*.ts{,x}",
    "postinstall": "yarn build",
    "start": "webpack-dev-server --inline --progress",
    "serve": "node server.js"
  },
  "babel": {
    "presets": [
      "stage-0",
      "react"
    ],
    "plugins": [
      "transform-strict-mode"
    ]
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "compression": "^1.7.2",
    "dotenv": "^5.0.1",
    "express": "^4.16.3",
    "express-enforces-ssl": "^1.1.0",
    "firebase": "^4.12.1",
    "lodash": "^4.17.5",
    "material-ui": "^1.0.0-beta.41",
    "react": "^16.3.1",
    "react-dom": "^16.3.1",
    "react-redux": "^5.0.7",
    "redux": "^3.7.2",
    "styled-components": "^3.2.6",
    "uglifyjs-webpack-plugin": "^1.2.4"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.3",
    "@types/lodash": "^4.14.107",
    "@types/react": "^16.3.9",
    "@types/react-dom": "^16.0.5",
    "@types/react-redux": "^5.0.16",
    "awesome-typescript-loader": "^5.0.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-strict-mode": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "css-loader": "^0.28.11",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "2.7.0",
    "eslint-plugin-jsx-a11y": "6.0.2",
    "eslint-plugin-react": "^7.7.0",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "style-loader": "^0.20.3",
    "svg-sprite-loader": "^3.7.3",
    "tslint": "^5.9.1",
    "tslint-config-airbnb": "^5.8.0",
    "typescript": "^2.8.1",
    "url-loader": "^1.0.1",
    "webpack": "^4.5.0",
    "webpack-cli": "^2.0.14",
    "webpack-dev-server": "^3.1.3"
  }
}
